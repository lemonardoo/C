#include<stdio.h>
int main(void)
{
    int a[10];
    int i,sum;
    double max,min,average;
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[0];
    min=a[0];
    sum=0;
    for(i=0;i<10;i++)
    {
        if(a[i]>max) max=a[i];
        if(a[i]<min) min=a[i];
        sum+=a[0];
    }
    average=sum/10;
    printf("最高分：%lf\n",max);
    printf("最低分：%lf\n",min);
    printf("平均分：%lf\n",average);
    return 0;
}
