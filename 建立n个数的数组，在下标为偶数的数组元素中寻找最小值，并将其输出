#include<stdio.h>
int main(void)
{
    int n;
    scanf("%d",&n);
    int a[n];
    int i,min;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    min=a[0];
    for(i=0;i<n;i+=2)
    {
        if(min>a[i])
        {
            min=a[i];
        }
    }
    printf("min = %d\n",min);
    return 0;
}
